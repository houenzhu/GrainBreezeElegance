<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhe.grain.mapper.AdminMapper">
    <resultMap id="AdminBaseMap" type="com.zhe.grain.entity.AdminEntity">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="username" property="username" javaType="String"/>
        <result column="password" property="password" javaType="String"/>
        <result column="phone" property="phone" javaType="String"/>
        <result column="slat" property="slat" javaType="String"/>
        <result column="avatar" property="avatar" javaType="String"/>
        <result column="age" property="age" javaType="INTEGER"/>
        <result column="gender" property="gender" javaType="INTEGER"/>
        <result column="create_time" property="createTime" javaType="java.util.Date"/>
        <result column="login_time" property="loginTime" javaType="java.util.Date"/>
    </resultMap>

</mapper>